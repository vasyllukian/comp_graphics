from tkinter import *
f = open("D:\\the work\sem3 exclusive\comp graphics and multimedia\lab2\DS7.txt", "r")
import tkinter as tk
root = Tk()
root.title("Лабораторна 2. Програмування. Лук'яненко")
root.geometry('960x540')
root.resizable(width = False, height = False)
xs = []
ys = []
for line in f:
    y,x = line.split(" ")
    xs.append(x)
    ys.append(y)
for i in range(len(xs)):
    label = Label(root, text = ".")
    label.place(x=xs[i],y=ys[i])
root.mainloop()